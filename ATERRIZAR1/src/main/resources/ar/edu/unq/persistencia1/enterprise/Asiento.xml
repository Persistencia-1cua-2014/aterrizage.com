<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ar.edu.unq.persistencia1.enterprise.asientos.Asiento" table="Asiento">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>

        <property name="usuario" type="ar.edu.unq.persistencia1.Usuario">
            <column name="usuario"  />
        </property>

        <property name="categoria" type="ar.edu.unq.persistencia1.enterprise.asientos.Categoria">
            <column name="categoria" not-null="true" />
        </property>

    </class>
</hibernate-mapping>